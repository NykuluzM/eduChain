<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="eduChain.Views.ContentPages.IpfsConnectPage"
             Title="IpfsConnectPage">
    <VerticalStackLayout>
        <Label 
            Text="Welcome to .NET MAUI!"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />
        <Button Text="Upload" Command="{Binding UploadCommand}"/>
       <Button Text="Download" Command="{Binding DownloadCommand}"/>
       <Button Text="Verify" Command="{Binding VerifyCommand}"/>
       <Button Text="List" Command="{Binding CheckCommand}"/>
    </VerticalStackLayout>
</ContentPage>